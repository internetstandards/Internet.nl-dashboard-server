---
targets:
- alias: live
  vars:
    env: live
  uri: dashboard.internet.nl
- alias: staging
  vars:
    env: staging
  uri: acc.dashboard.internet.nl
- alias: lab
  vars:
    env: lab
  config:
    ssh:
      user: vagrant
      private-key: ".vagrant/machines/default/virtualbox/private_key"
  uri: 172.30.1.5
